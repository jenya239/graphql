{
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "printWidth": 110,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "semi": false,
  "useTabs": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^../(.*)$",
    "^./(.*)$",
    "^(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "organizeImportsSkipDestructiveCodeActions": true
}